{
    "env": {
      "es2021": true,
      "node": true,
      "mocha": true
    },
    "extends": [
      "airbnb-base"
    ],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "rules": {
      "linebreak-style": "off",
      "no-underscore-dangle": "off",
      "class-methods-use-this": "off",
      "no-unused-vars": "off",
      "no-console": "off"
    },
    "import/no-extraneous-dependencies": [
      "error", {"devDependencies": true}
    ]
  }
  